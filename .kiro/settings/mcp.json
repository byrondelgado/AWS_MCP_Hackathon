{
    "mcpServers": {
        "awslabs.aws-documentation-mcp-server": {
            "command": "uvx",
            "args": [
                "awslabs.aws-documentation-mcp-server@latest"
            ],
            "env": {
                "FASTMCP_LOG_LEVEL": "ERROR",
                "AWS_DOCUMENTATION_PARTITION": "aws"
            },
            "disabled": false,
            "autoApprove": [
                "search_documentation",
                "read_documentation"
            ]
        },
        "fetch": {
            "command": "uvx",
            "args": [
                "mcp-server-fetch"
            ],
            "env": {},
            "disabled": false,
            "autoApprove": [
                "fetch"
            ],
            "disabledTools": []
        },
        "awslabs.amazon-bedrock-agentcore-mcp-server": {
            "command": "uvx",
            "args": [
                "awslabs.amazon-bedrock-agentcore-mcp-server@latest"
            ],
            "env": {
                "FASTMCP_LOG_LEVEL": "ERROR"
            },
            "disabled": false,
            "autoApprove": [
                "search_agentcore_docs",
                "fetch_agentcore_doc"
            ]
        },
        "strands-agents": {
            "command": "uvx",
            "args": [
                "strands-agents-mcp-server"
            ],
            "env": {
                "FASTMCP_LOG_LEVEL": "INFO"
            },
            "disabled": false,
            "autoApprove": [
                "search_docs",
                "fetch_doc"
            ]
        },
        "awslabs.cdk-mcp-server": {
            "command": "uvx",
            "args": [
                "awslabs.cdk-mcp-server@latest"
            ],
            "env": {
                "FASTMCP_LOG_LEVEL": "ERROR"
            },
            "disabled": false,
            "autoApprove": [
                "CDKGeneralGuidance",
                "GetAwsSolutionsConstructPattern",
                "SearchGenAICDKConstructs",
                "ExplainCDKNagRule"
            ]
        },
        "content-publishing-mcp": {
            "command": "python",
            "args": [
                "scripts/run_mcp_server.py"
            ],
            "env": {
                "MCP_LOG_LEVEL": "INFO",
                "MCP_ENABLE_ALL_TOOLS": "true"
            },
            "disabled": false,
            "autoApprove": [
                "get_brand_metadata",
                "list_publishers",
                "get_publisher_analytics",
                "calculate_trust_score_metrics",
                "create_publisher_profile",
                "get_branded_content",
                "validate_attribution",
                "track_brand_visibility"
            ]
        }
    }
}